<% content_for :title do %><%= @project.name %><% end %>
<div class="container projectview">
	<div class="center-align">
		<div class="row">
			<div class="col s12 m12 l12">
			<h4><%= @project.name %></h4> <br>
			<p class="chip"><%= @project.url %></p> <br>
			<%= image_tag @project.preview_url(:large), class: "materialboxed responsive-img " %>
			<%= @project.description.html_safe %> <br>
			</div>
		</div>	
	</div>
</div>